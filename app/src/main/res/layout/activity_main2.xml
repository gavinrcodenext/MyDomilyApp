<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity2">


    <TextView
        android:id="@+id/profile_one_label"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:textColor="#13100F"
        android:textSize="20sp"
        android:textStyle="bold"
        android:visibility="visible"
        android:gravity="center_horizontal"
        app:layout_constraintBottom_toTopOf="@+id/profile_button_one"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/profile_six_label"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:textColor="#13100F"
        android:textSize="20sp"
        android:textStyle="bold"
        android:visibility="invisible"
        android:gravity="center_horizontal"
        app:layout_constraintBottom_toTopOf="@+id/profile_button_six"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/profile_five_label"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:textColor="#13100F"
        android:textSize="20sp"
        android:textStyle="bold"
        android:visibility="invisible"
        android:gravity="center_horizontal"
        app:layout_constraintBottom_toTopOf="@+id/profile_button_five"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/profile_three_label"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:textColor="#13100F"
        android:textSize="20sp"
        android:textStyle="bold"
        android:visibility="invisible"
        android:gravity="center_horizontal"
        app:layout_constraintBottom_toTopOf="@+id/profile_button_three"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/profile_four_label"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:textColor="#13100F"
        android:textSize="20sp"
        android:textStyle="bold"
        android:visibility="invisible"
        android:gravity="center_horizontal"
        app:layout_constraintBottom_toTopOf="@+id/profile_button_four"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/profile_two_label"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:textColor="#13100F"
        android:textSize="20sp"
        android:textStyle="bold"
        android:visibility="invisible"
        android:gravity="center_horizontal"
        app:layout_constraintBottom_toTopOf="@+id/profile_button_two"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/activity3"
        android:textColor="#120E0E"
        android:textSize="24sp"
        android:textStyle="bold"
        android:gravity="center_horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.047" />

    <Button
        android:id="@+id/profile_button_one"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="50dp"
        android:foreground="@drawable/ic_personthing"
        android:onClick="goToChoreList"
        android:visibility="invisible"
        app:backgroundTint="#74BA77"
        app:iconPadding="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <Button
        android:id="@+id/profile_button_two"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="50dp"
        android:text=" "
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <Button
        android:id="@+id/profile_button_three"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <Button
        android:id="@+id/profile_button_four"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="50dp"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profile_button_one" />

    <Button
        android:id="@+id/profile_button_five"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="50dp"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profile_button_two" />

    <Button
        android:id="@+id/profile_button_six"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/profile_button_three" />

</androidx.constraintlayout.widget.ConstraintLayout>